depend() {
	use net
}

start() {
	ebegin "Starting saned"
	start-stop-daemon --start --exec /usr/sbin/saned -- -a saned
	eend $?
}

stop() {
	ebegin "Stopping saned"
	start-stop-daemon --stop --pidfile /var/run/saned.pid
	eend $?
}
